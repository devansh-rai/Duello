{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "concurrently \"npm run watch\" \"npm run server\" \"npm run watch-views\"",
    "server": "nodemon ./dist/index.js",
    "copy-files": "copyfiles -u 1 \"src/views/**/*\" dist/",
    "watch": "tsc -w",
    "watch-views": "nodemon --watch src/views -e ejs -x \"npm run copy-files\"",
    "build": "concurrently \"tsc\" \"npm run copy-files\" "
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.63.2",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "nodemailer": "^7.0.10",
    "prisma": "^6.19.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.5",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.3",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.11",
    "typescript": "^5.9.3"
  }
}
